funs.LCH_ValidIndex = (index) ->
    switch @ProductType()
        when "FRA" then index in (["AUD-BBR-BBSW","CAD-BA-CDOR","CHF-LIBOR-BBA","CZK-PRIBOR-PRBO","DKK-CIBOR2-DKNA13","EUR-LIBOR-BBA","EUR-EURIBOR-Reuters","GBP-LIBOR-BBA","HUF-BUBOR-Reuters","JPY-LIBOR-BBA","NOK-NIBOR-NIBR","NZD-BBR-FRA","SEK-STIBOR-SIDE","USD-LIBOR-BBA","PLN-WIBOR-WIBO","ZAR-JIBAR-SAFEX"] or [])
        when "OIS" then index in (["AUD-BBR-BBSW","CAD-CORRA-OIS-COMPOUND","CAD-BA-CDOR","CHF-LIBOR-BBA","CZK-PRIBOR-PRBO","CHF-TOIS-OIS-COMPOUND","DKK-CIBOR-DKNA13","DKK-CIBOR2-DKNA13","EUR-EONIA-OIS-COMPOUND","EUR-LIBOR-BBA","EUR-EURIBOR-Reuters","EUR-EURIBOR-Telerate","GBP-LIBOR-BBA","GBP-WMBA-SONIA-COMPOUND","HKD-HIBOR-HIBOR=","HKD-HIBOR-HKAB","HKD-HIBOR-ISDC","JPY-LIBOR-BBA","JPY-TONA-OIS-COMPOUND","NOK-NIBOR-NIBR","NZD-BBR-FRA","NZD-BBR-Telerate","PLN-WIBOR-WIBO","PLZ-WIBOR-WIBO","SEK-STIBOR-SIDE","USD-Federal Funds-H.15-OIS-COMPOUND","USD-LIBOR-BBA","ZAR-JIBAR-SAFEX"] or [])
        else not index in (["GBP-WMBA-RONIA-COMPOUND","JPY-TIBOR-TIBM (5 Banks)","JPY-TIBOR-TIBM (10 Banks)","JPY-TIBOR-ZTIBOR","USD-SIFMA Municipal Swap Index","USD-TBILL-H.15","USD-CP-H.15","USD-Federal Funds-H.15","USD-Prime-H.15","SGD-SIBOR-Reuters","SGD-SOR-Telerate","AUD-LIBOR-BBA","CAD-LIBOR-BBA"] or [])
